{
  "name": "Github repository's Dashboard",
  "version": "1.0",
  "description": "Github repository's Dashboard!",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["*://github.com/"],
      "js": ["./src/content.tsx"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["cookies", "storage"]
}
